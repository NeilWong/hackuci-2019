{
    "manifest_version": 2,
    "name": "Job Buddy",
    "version": "1.0",
    "description": "Save job listing details to a spreadsheet when you click apply!",
    "browser_action": {
      "icons": ["icon.png"],
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "content_scripts": [ {
      "js": [ "jquery.min.js", "background.js" ],
      "matches": [ "http://*/*", "https://*/*"]
    }],
    "web_accessible_resources": ["icon.png", "icon-full.png"],
    "permissions": ["storage"]
}